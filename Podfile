# platform :ios, '9.0'

target 'LooisKit' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for LooisKit
  pod 'web3swift', :git => 'https://github.com/LOOIS-IO/web3swift.git'

  target 'LooisKitTests' do
    inherit! :search_paths
    # Pods for testing
    
    pod 'Quick'
    pod 'Nimble'
  end

end
